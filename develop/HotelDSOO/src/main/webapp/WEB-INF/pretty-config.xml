<pretty-config xmlns="http://ocpsoft.org/schema/rewrite-config-prettyfaces" 
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
               xsi:schemaLocation="http://ocpsoft.org/schema/rewrite-config-prettyfaces
                      http://ocpsoft.org/xml/ns/prettyfaces/rewrite-config-prettyfaces.xsd">
    <url-mapping id="index"> 
        <pattern value="/" /> 
        <view-id value="/views/public/index.jsf" />
    </url-mapping>
    
    <url-mapping id="reserve"> 
        <pattern value="/reserve" /> 
        <view-id value="/views/public/reserve.jsf" />
    </url-mapping>
    
    <url-mapping id="cancelReserve"> 
        <pattern value="/cancelReserve" /> 
        <view-id value="/views/public/cancelReserve.jsf" />
    </url-mapping>
    
    <url-mapping id="checkReserve"> 
        <pattern value="/checkReserve" /> 
        <view-id value="/views/public/checkReserve.jsf" />
    </url-mapping>
</pretty-config>